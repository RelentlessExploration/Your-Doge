{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "license": "GPL-3.0-or-later",
  "main": "public/electron.js",
  "author": "Tim",
  "description": "An awesome doge themed homework manager",
  "dependencies": {
    "electron-is-dev": "^2.0.0"
  },
  "scripts": {
    "dev:electron": "concurrently -k \"BROWSER=none yarn dev\" \"yarn electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "yourdoge",
    "productName": "Your Doge",
    "copyright": "Tim",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "resources/icon.png"
    },
    "win": {
      "icon": "resources/icon.png"
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@graphql-codegen/typescript-react-apollo": "3.0.0",
    "@types/cookie": "^0.4.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.3.1",
    "@types/react-bootstrap": "^0.32.26",
    "@types/react-calendar": "^3.4.1",
    "@types/react-google-recaptcha": "^2.1.2",
    "@types/react-router-dom": "^5.1.8",
    "electron": "13.1.8",
    "electron-builder": "^22.11.7",
    "eslint-config-next": "^11.0.1",
    "graphql-tag": "^2.0.0",
    "wait-on": "5.1.0"
  }
}
